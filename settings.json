{
    "name": "gws_stats",
    "author": "Gencovery",
    "version": "0.1.0",
    "variables": {
        "gws_stats:testdata_dir": "{CURRENT_DIR}/tests/testdata"
    },
    "environment": {
        "pip": [
            {
                "source": "https://pypi.python.org/simple",
                "packages": []
            },
            {
                "source": "https://$GWS_PIP_PACKAGE_TOKEN@gitlab.com/api/.../packages/pypi",
                "packages": []
            }
